@article{solariConstructionQuantumMechanics,
  title = {The Construction of Quantum Mechanics from Electromagnetism. {{Part I}}: The Unitary Entity.},
  author = {Solari, Hern{\'a}n Gustavo and Natiello, Mario Alberto},
  abstract = {We reconstruct Quantum Mechanics in a way that harmonises with Classical Mechanics and Electromagnetism, free from mysteries or paradoxes as the collapse of the wave-function or Schr\"odinger's cat. The construction is inspired by de Broglie and Schr\"odinger's wave mechanics while the unifying principle is Hamilton's principle of least action, which separates natural laws from particular circumstances such as initial conditions and leads to the conservation of energy for isolated systems. We also provide a criticism of instrumentalism, the leading philosophical perspective behind conventional Quantum Mechanics.},
  langid = {english}
