@misc{araiLargeLanguageModels2024,
  title = {Do {{Large Language Models Defend Inferentialist Semantics}}?: {{On}} the {{Logical Expressivism}} and {{Anti-Representationalism}} of {{LLMs}}},
  shorttitle = {Do {{Large Language Models Defend Inferentialist Semantics}}?},
  author = {Arai, Yuzuki and Tsugawa, Sho},
